{{# each product}}
  <div class="row">
    {{# each this}}
    <div class="col-4">
      <div class="card picture" style="width: 18rem;">
        <img src="{{this.imagePath}}" class="card-img-top" alt="..." height="200px">
        <div class="card-body">
          <h5 class="card-title">{{this.title}}</h5>
          <p class="card-text">{{this.description}}</p>
          <div class="clearfix">
              <div class="price pull-left"><b>${{this.price}}</b></div>
              <a href="/add-to-cart/{{this._id}}" class="btn btn-success pull-right">Add To Cart</a>
          </div>
          
        </div>
    </div>
  </div>  

    {{/each}}
  </div>
  <br>
{{/each}}



